{
	"name": "SmartComments",
	"author": [
		"Youri van den Bogert",
		"Erwin Oord",
		"Robin van der Wiel",
		"Yvar Nanlohij"
	],
	"version": "2.0.5",
	"url": "https://www.archixl.nl",
	"descriptionmsg": "sic-desc",
	"license-name": "GPL-2.0+",
	"type": "other",
	"MessagesDirs": {
		"SmartComments": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SmartComments\\": "src"
	},
	"SpecialPages": {
		"SmartComments": "MediaWiki\\Extension\\SmartComments\\SpecialPage\\Special"
	},
	"JobClasses": {
		"SmartCommentsUpdateJob": "MediaWiki\\Extension\\SmartComments\\Jobs\\Update"
	},
	"APIModules": {
		"smartcomments": "MediaWiki\\Extension\\SmartComments\\Api"
	},
	"Actions": {
		"screfresh": "MediaWiki\\Extension\\SmartComments\\RefreshAction"
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\SmartComments\\SchemaUpdates::onLoadExtensionSchemaUpdates",
		"BeforePageDisplay": "MediaWiki\\Extension\\SmartComments\\Hooks::onBeforePageDisplay",
		"SkinAfterBottomScripts": "MediaWiki\\Extension\\SmartComments\\Hooks::onSkinAfterBottomScripts",
		"SkinTemplateNavigation::Universal": "MediaWiki\\Extension\\SmartComments\\Hooks::onSkinTemplateNavigation",
		"ResourceLoaderGetConfigVars": "MediaWiki\\Extension\\SmartComments\\Hooks::onResourceLoaderGetConfigVars",
		"SMW::Property::initProperties": "MediaWiki\\Extension\\SmartComments\\SMWHandler::initProperties",
		"SMW::SQLStore::BeforeDataUpdateComplete": "MediaWiki\\Extension\\SmartComments\\Hooks::addSubobjectsForComments",
		"ParserCacheSaveComplete": "MediaWiki\\Extension\\SmartComments\\Hooks::onParserCacheSaveComplete",
		"MediaWikiServices": "MediaWiki\\Extension\\SmartComments\\Hooks::onMediaWikiServices",
		"ArticleDeleteAfterSuccess": "MediaWiki\\Extension\\SmartComments\\Hooks::onArticleDeleteAfterSuccess"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "SmartComments"
	},
	"ResourceModules": {
		"ext.smartcomments.special": {
			"styles": [
				"resources/special/less/smartcomments.less"
			],
			"scripts": [
				"resources/special/js/smartcomments.js"
			],
			"messages": [
				"sic-sp-adminbutton-confirm"
			]
		},
		"ext.smartcomments.vue-demi": {
			"scripts": [
				"resources/frontend/upstream/vue-demi.iife.js"
			]
		},
		"ext.smartcomments.pinia": {
			"scripts": [
				"resources/frontend/upstream/pinia.iife.js",
				"resources/frontend/js/piniaWrapper.js"
			],
			"dependencies": [
				"vue",
				"ext.smartcomments.vue-demi"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.smartcomments.frontend": {
			"packageFiles": [
				"resources/frontend/js/App.js",
				"resources/frontend/js/directives/highlightDirective.js",
				"resources/frontend/js/composables/useHighlight.js",
				"resources/frontend/js/composables/useComments.js",
				"resources/frontend/js/composables/useSelection.js",
				"resources/frontend/js/composables/useSelectionEvents.js",
				"resources/frontend/js/composables/useScreenshot.js",
				"resources/frontend/js/utils/constants.js",
				"resources/frontend/js/utils/selectionUtils.js",
				"resources/frontend/js/utils/smartCommentsEvents.js",
				"resources/frontend/js/utils/vueDemiPolyfill.js",
				"resources/frontend/js/composables/useSmartCommentsSetup.js",
				"resources/frontend/js/components/ReplyForm.vue",
				"resources/frontend/js/components/ReplyList.vue",
				"resources/frontend/js/components/Comment.vue",
				"resources/frontend/js/components/CommentActions.vue",
				"resources/frontend/js/components/CommentBody.vue",
				"resources/frontend/js/components/CommentMessage.vue",
				"resources/frontend/js/components/NewCommentDialog.vue",
				"resources/frontend/js/components/CommentTimeline.vue",
				"resources/frontend/js/store/appStateStore.js",
				"resources/frontend/js/store/commentsStore.js",
				"resources/frontend/js/store/userStore.js",
				"resources/frontend/js/SmartComments.vue"
			],
			"styles": [
				"resources/frontend/less/smartcomments.less"
			],
			"dependencies": [
				"mediawiki.api",
				"mediawiki.user",
				"ext.smartcomments.upstream",
				"ext.smartcomments.pinia"
			]
		},
		"ext.smartcomments.upstream": {
			"scripts": [
				"resources/js/node_modules/rangy/lib/rangy-core.js",
				"resources/js/node_modules/rangy/lib/rangy-classapplier.js",
				"resources/js/node_modules/rangy/lib/rangy-highlighter.js",
				"resources/js/node_modules/rangy/lib/rangy-serializer.js",
				"resources/js/node_modules/rangy/lib/rangy-textrange.js",
				"resources/js/node_modules/rangy/lib/rangy-selectionsaverestore.js",
				"resources/js/node_modules/html2canvas/dist/html2canvas.js"
			]
		}
	},
	"AvailableRights": [
		"add-inlinecomments",
		"manage-inlinecomments"
	],
	"GroupPermissions": {
		"user": {
			"add-inlinecomments": true,
			"manage-inlinecomments": false
		},
		"sysop": {
			"add-inlinecomments": true,
			"manage-inlinecomments": true
		}
	},
	"config": {
		"SmartCommentsSelectLinksOnClick": {
			"value": true
		},
		"SmartCommentsDirectComment": {
			"value": true
		},
		"SmartCommentsSpecialMaxItems": {
			"value": 25
		}
	},
	"requires": {
		"MediaWiki": ">= 1.39"
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}